{
  "expo": {
    "name": "Réseau des Colibris du Compiégnois",
    "slug": "rcc",
    "description": "Découvrez les actions du Réseau des Colibris du Compiégnois et suivez celles qui vous intéressent",
    "scheme": "rcc",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rcc/icon.png",
    "primaryColor": "#bdc900",
    "splash": {
      "image": "./assets/rcc/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/rcc/icon-notification.png",
      "color": "#bdc900"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "app.reconnexion.rcc",
      "googleServicesFile": "./google-services.json",
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "app.reconnexion.rcc",
      "icon": "./assets/rcc/icon-no-transparency.png",
      "supportsTablet": false
    }
  }
}
